{
    "name": "kudosagency/companieshouse",
    "description": "A Laravel wrapper for the companies house API",
    "license": "Unlicense",
    "authors": [
        {
            "name": "Trevor Sewell",
            "email": "trev@kudos.agency"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "files": [
            "src\\Helpers\\TestHelpers.php"
        ],
        "psr-4": {
            "KudosAgency\\CompaniesHouse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KudosAgency\\CompaniesHouse\\Providers\\ServiceProvider"
            ],
            "aliases": {
                "CompaniesHouse": "KudosAgency\\CompaniesHouse\\Facades\\CompaniesHouse"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    }
}
